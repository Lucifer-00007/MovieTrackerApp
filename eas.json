{
  "cli": {
    "version": ">= 16.18.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_PUBLIC_USE_MOCK_DATA": "false",
          "EXPO_PUBLIC_DISABLE_ANALYTICS": "true",
          "EXPO_PUBLIC_API_PROVIDER": "cloudflare"
        }
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}